---
title: "Search Index"
layout: null
permalink: /search.json
---
[
{% assign docs = site.pages | concat: site.posts | concat: site.writeups | concat: site.projects %}
{% for doc in docs %}
  {
    "title": {{ doc.title | jsonify }},
    "url": "{{ doc.url | relative_url }}",
    "date": "{{ doc.date | date: "%Y-%m-%d" }}",
    "content": {{ doc.content | strip_html | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
