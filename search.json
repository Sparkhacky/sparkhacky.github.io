---
layout: null
permalink: /search.json
---
{
  "posts": [
    {% for p in site.posts %}
    {
      "id": "post-{{ forloop.index }}",
      "title": {{ p.title | jsonify }},
      "url": {{ p.url | relative_url | jsonify }},
      "type": "briefing",
      "date": "{{ p.date | date_to_string }}",
      "tags": {{ p.tags | jsonify }},
      "categories": {{ p.categories | jsonify }},
      "excerpt": {{ p.excerpt | strip_html | normalize_whitespace | jsonify }},
      "content": {{ p.content | strip_html | normalize_whitespace | jsonify }}
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ],
  "writeups": [
    {% for w in site.writeups %}
    {
      "id": "w-{{ forloop.index }}",
      "title": {{ w.title | jsonify }},
      "url": {{ w.url | relative_url | jsonify }},
      "type": "writeup",
      "date": "{{ w.date | date_to_string }}",
      "tags": {{ w.tags | jsonify }},
      "categories": {{ w.categories | jsonify }},
      "excerpt": {{ w.excerpt | strip_html | normalize_whitespace | jsonify }},
      "content": {{ w.content | strip_html | normalize_whitespace | jsonify }}
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
